# Environment variables template
# Copy this file to .env.local and update the values

# API Base URL for your backend
VITE_API_BASE_URL=http://localhost:8000/api

# Health check URL (without /api)
VITE_API_HEALTH_URL=http://localhost:8000

# For production, update these to your deployed backend URLs:
# VITE_API_BASE_URL=https://your-app-name.vercel.app/api
# VITE_API_HEALTH_URL=https://your-app-name.vercel.app
